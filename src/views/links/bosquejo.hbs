<form action="/links/profile" class="btn-group dropup btn-float fs-2 m-2" style="left: 5px;">
  <button class="fa-solid fa-house fa-flip bg-transparent" style="border: none;color: rgb(231, 14, 14);" ></button>
</form>


<main>
  <div class="container d-flex justify-content-center mt-5" style="margin-bottom: 150px;">
    <div
      class="card bg-body-secondary"
      style="width: 30rem; box-shadow: 10px 10px 5px 0px rgba(54,130,123,0.75);"
    >
      <div class="d-flex justify-content-center m-2">
        <img src="/img/logo.png" alt="logo" class="logo-form" />
      </div>
       <form action="/links/bosquejo" method="post" enctype="multipart/form-data">
          <div class="form-group p-2">
            <label for="fileUpload" 
            class="form-label">Selecciona un archivo para subir</label>
            <input
              type="file"
              id="fileUpload"
              class="form-control"
              style="border-bottom: 2px solid #5504f8;"
              name="fileUpload"
              required
            />
            <button type="submit" class="btn btn-primary btn-lg m-2" >Cargar Bosquejo</button>
          </div>
          
        </form>

      </div>
    </div>
  </div>
</main>

